#
#  mod_rewrite active
#
#
#  Files
#  .*\.ini .*\.log .*\.cgi

<Files ~ "^\.ht">
    Order allow,deny
    Deny from all
    Satisfy All
</Files>

<Files ~ "^(.+)\.ini">
    Order allow,deny
    Deny from all
    Satisfy All
</Files>

<Files ~ "^(.+)\.log">
    Order allow,deny
    Deny from all
    Satisfy All
</Files>

<Files ~ "^(.+)\.cgi">
    Order allow,deny
    Deny from all
    Satisfy All
</Files>

<Files ~ "^(.+)\.bak">
    Order allow,deny
    Deny from all
    Satisfy All
</Files>

